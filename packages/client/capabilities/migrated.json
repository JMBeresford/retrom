{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "remote": {
    "urls": ["https://*", "http://localhost:*"]
  },
  "windows": ["main", "emulator-js"],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:window:allow-close",
    "core:app:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "core:webview:allow-create-webview-window",
    "core:webview:allow-webview-close",
    "fs:default",
    "fs:deny-default",
    "fs:allow-appdata-write-recursive",
    {
      "identifier": "fs:allow-exists",
      "allow": [
        {
          "path": "$APPDATA/*"
        },
        {
          "path": "$APPDATA"
        }
      ]
    },
    {
      "identifier": "fs:allow-mkdir",
      "allow": [
        {
          "path": "$APPDATA"
        }
      ]
    },
    "installer:allow-install-game",
    "installer:allow-uninstall-game",
    "installer:allow-get-game-installation-status",
    "installer:allow-get-installation-state",
    "installer:allow-open-installation-dir",
    "installer:allow-migrate-installation-dir",
    "installer:allow-clear-installation-dir",
    "installer:allow-update-steam-installations",
    "dialog:allow-open",
    "launcher:allow-play-game",
    "launcher:allow-stop-game",
    "launcher:allow-get-game-play-status",
    "updater:default",
    "shell:allow-open",
    "process:default",
    "system-info:allow-all-sys-info",
    "core:window:allow-set-fullscreen",
    "window-state:default",
    "standalone:allow-enable-standalone-mode",
    "standalone:allow-disable-standalone-mode",
    "config:allow-get-config",
    "config:allow-set-config"
  ]
}
